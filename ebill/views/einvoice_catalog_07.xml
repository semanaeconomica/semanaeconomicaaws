<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_einvoice_catalog_07_form" model="ir.ui.view">
		<field name="name">einvoice.catalog.07.form</field>
		<field name="model">einvoice.catalog.07</field>
		<field name="arch" type="xml">
			<form string="Einvoice Catalog 07">
				<sheet>
					<group>
						<group>
							<field name="code" required="1"/>
							<field name="name" required="1"/>
						</group>
						<group>
							<field name="tribute_code" required="1"/>
							<field name="pse_code"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>
	<record id="view_einvoice_catalog_07_list" model="ir.ui.view">
		<field name="name">einvoice.catalog.07.list</field>
		<field name="model">einvoice.catalog.07</field>
		<field name="arch" type="xml">
			<tree>
				<field name="code" optional="show"/>
				<field name="name" optional="show"/>
				<field name="tribute_code" optional="show"/>
				<field name="pse_code" optional="show"/>
			</tree>
		</field>
	</record>
	<record id="action_einvoice_catalog_07_form" model="ir.actions.act_window">
		<field name="name">Catalogo 07 - Codigo de tipo de Afectacion del IGV</field>
		<field name="res_model">einvoice.catalog.07</field>
		<field name="view_mode">tree,form</field>
		<field name="view_id" ref="view_einvoice_catalog_07_list"/>
		<field name="help" type="html">
		  <p class="o_view_nocontent_smiling_face">
			Crea tu primera tabla de Catalogo 07 aqui
		  </p>
		</field>
	</record>
	<menuitem action="action_einvoice_catalog_07_form" id="menu_action_einvoice_catalog_07_form" parent="account_base_it.menu_account_base_it" sequence="10" groups="account_base_it.group_account_tables_user"/>
</odoo>